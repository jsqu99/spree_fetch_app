<% html_str = ""%>
<% if flash[:error] %>
  <%  html_str = "<span class='flash error'>#{t(:fetchapp_synch_error)}: #{flash[:error]}</span>" %>
<% else %>
  <%  html_str = "<span class='flash notice'>#{t(:fetchapp_synch_completed)}</span>" %>
<% end %>


$('#fetchapp_synch_notification').html("<%= html_str.html_safe %> ");
